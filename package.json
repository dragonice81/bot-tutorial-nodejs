{
  "name": "GarrettBot",
  "version": "2.0.1",
  "description": "A Groupme chatbot",
  "keywords": [
    "express",
    "heroku",
    "node"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dragonice81/garrett-bot"
  },
  "license": "MIT",
  "main": "server.js",
  "scripts": {
    "deploy": "heroku container:push web --app garrettbot-docker && heroku container:release web --app garrettbot-docker",
    "eslint": "eslint '**/*.js' --fix",
    "start": "node server.js",
    "start:dev": "node_modules/.bin/nodemon --inspect=0.0.0.0:9228 server.js",
    "start:frontend": "cd public/web-player/ && npm install --quiet && npm start"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "prettier --write",
      "git add"
    ],
    "**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "body-parser": "1.18.3",
    "color-to-name": "1.0.15",
    "config": "3.0.1",
    "dotenv": "6.2.0",
    "eightball": "1.0.1",
    "express": "4.16.4",
    "get-port": "4.1.0",
    "lodash": "4.17.11",
    "markov-strings": "2.0.1",
    "node-schedule": "1.3.1",
    "request-promise-native": "1.0.5",
    "weather-js": "2.0.0",
    "text-meme": "2.3.0",
    "fs-readfile-promise": "3.0.1",
    "express-async-errors": "3.1.1",
    "winston": "3.1.0",
    "nostra": "0.0.7",
    "spotify-web-api-node": "4.0.0",
    "urban-dictionary": "2.2.1"
  },
  "devDependencies": {
    "@types/dotenv": "4.0.3",
    "@types/body-parser": "1.17.0",
    "@types/config": "0.0.34",
    "@types/express": "4.16.1",
    "@types/lodash": "4.14.123",
    "@types/node-schedule": "1.2.2",
    "@types/request-promise-native": "1.0.15",
    "@types/fs-readfile-promise": "3.0.0",
    "tslint": "5.14.0",
    "tslint-no-unused-expression-chai": "0.1.4",
    "prettier": "1.16.4",
    "nodemon": "1.18.9",
    "pre-commit": "1.2.2",
    "typescript": "3.4.1",
    "husky": "1.3.1",
    "lint-staged": "8.1.5"
  },
  "engines": {
    "node": "11.0.0"
  }
}
